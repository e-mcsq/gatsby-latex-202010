  "scripts": {
    "build": "yarn create-ts-types && yarn build-www",
    "build-www": "gatsby build",
    "develop": "yarn create-ts-types && yarn develop-www",
    "develop-www": "gatsby develop -p 3000 -H 0.0.0.0",
    "start": "yarn develop",
    "serve": "gatsby serve -p 3001 -H 0.0.0.0",
    "preserve": "yarn build",
    "create-ts-types": "node ./src/schemata/compileJsonSchemataToTypescript.js"
  },
